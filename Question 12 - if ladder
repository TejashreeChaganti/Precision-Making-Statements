#include <iostream>
using namespace std;

int main() {
    int choice;
    float length, breadth;
    cout << "Enter 3 to calculate area of rectangle: ";
    cin >> choice;

    if (choice == 1)
        cout << "Option 1 not implemented." << endl;
    else if (choice == 2)
        cout << "Option 2 not implemented." << endl;
    else if (choice == 3) {
        cout << "Enter length and breadth: ";
        cin >> length >> breadth;
        cout << "Area of rectangle: " << length * breadth << endl;
    } else
        cout << "Invalid input." << endl;

    return 0;
}
