#include <iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Enter two numbers: ";
    cin >> a >> b;

    if (a > b)
        cout << "Maximum is: " << a;
    else if (b > a)
        cout << "Maximum is: " << b;
    else
        cout << "Both numbers are equal.";

    return 0;
}
